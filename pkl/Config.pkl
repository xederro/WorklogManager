/// Config is the main configuration for the WorklogManager application.
@go.Package { name = "github.com/xederro/WorklogManager/internal/gen/config" }
module Config

import "package://pkg.pkl-lang.org/pkl-go/pkl.golang@0.10.0#/go.pkl"
import "JiraConfig.pkl"
import "GoogleAIConfig.pkl"

/// The path to database.
db_path: String = "./db/db.sqlite"
jira: JiraConfig
use_ai: Boolean = false
google_ai: GoogleAIConfig?((use_ai == true).implies(this != null))