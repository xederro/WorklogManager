amends ".../Config.pkl"

jira {
  url = "https://your-jira-instance.atlassian.net"
  refetch_interval = 15.min
  default_worklog_comment = "Work"
  server_type = "cloud"
  cloud_config {
    email = "email@example.com"
    api_token = "your_api_token"
  }
}
use_ai = true
google_ai {
  APIKey = "your_google_api_key"
}